{
  "name": "Door Key | VQVAE | Best",

  "project": "model-free-minigrid-online",

  "command": "python model_free/train.py",

  "algorithm": "grid",

  "spec": {
    "metric": "reward",
    "objective": "maximize",
    "retryAssignLimit": 4
  },

  "trials": 30,

  "parameters": {
    "comet_ml": True,
    "ae_recon_loss": True,
    "ae_er_train": True,
    "count": True,
    "device": "cuda",
    "log_freq": 10000,
    "learning_rate": 0.00003,
    "batch_size": 256,
    "mf_steps": 1000000,
    "embedding_dim": 64,
    "env_max_steps": 1000,
    "n_ae_updates": 8,
    "ppo_batch_size": 64,
    "ppo_value_coef": 0.5,
    "ppo_iters": 10,
    "ppo_clip": 0.2,

    "ae_model_type": "vqvae",
    "filter_size": 6,
    "codebook_size": 256,

    "agg_func_const": 100,

    "env_name": "minigrid-door-key-stochastic",
    "stochastic": "categorical",

    "beta": {
        "type": "discrete",
        "values": [0.0, 0.001, 0.01]
      }

  }
}
